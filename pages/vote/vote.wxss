page {
  min-height: 100%;
  width: 100%;
}
.row{
  display: flex;
  width:100%;
  height:100%;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.col-space{
  display: flex;
  width:50rpx;
  flex-direction: column;
}

.text-line{
  width:650rpx;
  border:1rpx solid #F4F6F9;
  border-left-width:0px;
  border-right-width:0px;
  border-top-width:0px;
  background-color: #FFF;
  height: 64rpx;
  padding-bottom: 22rpx;
}
.item{
  width:100%;
  display: flex;
  flex-direction: row;
}
.col-bot{
  width:100%;
  height:100%;
  display: flex;
  flex-direction: column;
  align-content: space-between;
  justify-content: center;
}

.item-bot{
  width:100%;
  height: 116rpx;
  display: flex;
  flex-direction: row;
  align-content: center;
  justify-content: space-between;

}
.font-place{
  color:#DBDEE4;
  letter-spacing: 0;
}

.title{
  font-size:  24px;
  font-weight:600
}
.des{
  font-size:  18px
}
.collect{
  font-size:  16px
}
.collect-pic{
  margin-top:6rpx;
  font-size: 13px;
  color: #5B739D;
  letter-spacing: 0;
}
.submit-btn{
  width:302rpx;
  height: 86rpx;
  background: #FDD64F;
  border-radius: 4px;
  /* border:0px solid rgba(0, 0, 0, 0) */
}
.foot{
  display: flex;
  width: 100%;
  height: 100%;
  background-color: #F9FAFA;
  flex-direction: row;
  justify-content: center;
}
.space{
  margin-top: 80rpx;
}
.margin{
  margin-left: 50rpx;
  margin-right:44rpx;
  margin-top:36rpx;
}
.height{
  height: 116rpx
}
.hide{
    display:none
}